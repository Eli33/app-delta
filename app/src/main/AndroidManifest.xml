<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jumpers.delta.sistemasparainter.net.appdelta">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CadastroActivity" />
        <activity android:name=".CadastroEnderecoActivity" />
        <activity android:name=".ProdutoActivity" />
        <activity android:name=".CarrinhoActivity" />
        <activity android:name=".PagamentoActivity" />
        <activity android:name=".FinalizacaoActivity" />
        <activity android:name=".LoginActivity"></activity>
    </application>

</manifest>